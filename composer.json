{
  "name": "custom/event_registration",
  "type": "drupal-module",
  "description": "A custom Drupal 10 module that allows users to register for events via a custom form, stores registrations, and sends email notifications.",
  "keywords": [
    "drupal",
    "event",
    "registration",
    "form",
    "email"
  ],
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Drupal Developer",
      "role": "Developer"
    }
  ],
  "homepage": "https://www.drupal.org/project/event_registration",
  "support": {
    "issues": "https://www.drupal.org/project/issues/event_registration",
    "source": "https://github.com/example/event_registration"
  },
  "require": {
    "php": "^8.1",
    "drupal/core": "^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\event_registration\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    },
    "drupal": {
      "version": "1.0.0"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}